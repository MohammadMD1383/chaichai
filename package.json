{
	"author": "Mohammad Mostafa Dastjerdi",
	"name": "chai-compat",
	"type": "module",
	"description": "fork of original chai compatible with both esm and cjs",
	"keywords": [
		"test",
		"assertion",
		"assert",
		"testing",
		"chai"
	],
	"homepage": "http://chaijs.com",
	"license": "MIT",
	"contributors": [
		"Jake Luer <jake@alogicalparadox.com>",
		"Domenic Denicola <domenic@domenicdenicola.com> (http://domenicdenicola.com)",
		"Veselin Todorov <hi@vesln.com>",
		"John Firebaugh <john.firebaugh@gmail.com>"
	],
	"version": "5.1.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/MohammadMD1383/chaichai"
	},
	"bugs": {
		"url": "https://github.com/chaijs/chai/issues"
	},
	"exports": {
		".": {
			"import": "./chai.js",
			"require": "./cjs/dist/index.js"
		}
	},
	"scripts": {
		"prebuild": "npm run clean",
		"build": "npm run build:esm && npm run build:cjs && cp -f cjs/loupe.json cjs/dist/node_modules/loupe/package.json",
		"build:esm": "esbuild --bundle --format=esm --keep-names --outfile=chai.js index.js",
		"build:cjs": "tsc -p tsconfig.json",
		"pretest": "npm run lint && npm run build",
		"test": "npm run test-node && npm run test-chrome",
		"test-node": "mocha --require ./test/bootstrap/index.js --reporter dot test/*.js",
		"test-chrome": "web-test-runner --playwright",
		"lint": "eslint lib/",
		"clean": "rm -f chai.js coverage"
	},
	"engines": {
		"node": ">=12"
	},
	"dependencies": {
		"assertion-error": "^2.0.1",
		"check-error": "^2.1.1",
		"deep-eql": "^5.0.1",
		"loupe": "^3.1.0",
		"pathval": "^2.0.0"
	},
	"devDependencies": {
		"typescript": "^5.5.3",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@web/dev-server-rollup": "^0.6.1",
		"@web/test-runner": "^0.18.0",
		"@web/test-runner-playwright": "^0.11.0",
		"esbuild": "^0.19.10",
		"eslint": "^8.56.0",
		"eslint-plugin-jsdoc": "^48.0.4",
		"mocha": "^10.2.0"
	}
}
